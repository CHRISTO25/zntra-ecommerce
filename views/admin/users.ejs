    <main class="main-wrap">
       
        <section class="content-main">
        <div class="card-body">
            <div class="table-responsive">
                <script src="https://code.jquery.com/jquery-3.7.0.js"></script>
                <script src="https://cdn.datatables.net/1.13.5/js/jquery.dataTables.min.js"></script>
                <table id="example" class="display" style="width:100%">
                    <thead>
                        <tr>
                            <!-- <th>Seller</th> -->
                            <th>Name</th>
                            <th>Email</th>
                            <th>Number</th>
                            <th>Username</th>
                            <th class="text-end"> Action </th>
                        </tr>
                    </thead>
                    <tbody>


                        <% data.forEach(x=>{%>
                        <tr>
                        
                            <td><%=x.name%></td>
                            <td><%=x.email%></td>
                            <td><%=x.number%></td>
                            <td><span class="badge rounded-pill alert-success"><%=x.username%></span></td>
                            <% if(x.varified==true){%>
                                <td class="text-end">
                                    <a href="/blk_unblk?id=<%=x._id%>" style="background-color: green; width: 68px;" class="btn btn-sm btn-brand rounded font-sm mt-15">Block</a>
                                </td>
                                <% }
                                else{%>
                                    <td class="text-end">
                                        <a href="/blk_unblk?id=<%=x._id%>" style="background-color:red;" class="btn btn-sm btn-brand rounded font-sm mt-15">Unblock</a>
                                    </td>
                                    <% }%>
                        </tr>
                        <%})%> 
                        <script>
                            new DataTable('#example', {
    search: {
        return: true
    }
});
                        </script>
                    </tbody>
                   
                    
                </table> <!-- table-responsive.// -->
               
                    
                <script>
     $(document).ready(function () {
    $('#example').DataTable();
});
                </script>
