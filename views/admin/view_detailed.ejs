<!DOCTYPE HTML>
<html lang="en">


<!-- Mirrored from wp.alithemes.com/html/evara/evara-backend//dash by HTTrack Website Copier/3.x [XR&CO'2014], Sun, 01 Aug 2021 15:32:35 GMT -->

<head>
    <meta charset="utf-8">
    <title>zntra Dashboard</title>
    <meta http-equiv="x-ua-compatible" content="ie=edge">
    <meta name="description" content="">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta property="og:title" content="">
    <meta property="og:type" content="">
    <meta property="og:url" content="">
    <meta property="og:image" content="">
    <!-- Favicon -->
    <link rel="shortcut icon" type="image/x-icon" href="static/admin/assets/imgs/icons/logo.png">
    <!-- Template CSS -->
    <link href="static/admin/assets/css/main.css" rel="stylesheet" type="text/css" />
</head>

<body>
    <%-include('nav')-%>
    <main class="main-wrap">
       
        <section class="content-main">
            <div class="content-header">
                <div>
                    <h2 class="content-title card-title">Products List</h2>
                    <p style="color: green;">
                        <%=er%>
                    </p>
                </div>
                <div>
                    <a href="#" class="btn btn-light rounded font-md">Export</a>
                    <a href="#" class="btn btn-light rounded  font-md">Import</a>
                    <a href="/order_view_admin" class="btn btn-primary btn-sm rounded">view order</a>
                </div>
            </div>
            <div class="card mb-4">
                <header class="card-header">
                </header> <!-- card-header end// -->
                <div class="table-responsive">

                    <table class="table">
                        <thead>
                            <tr>
                                <th width="40%">IMAGE/NAME</th>
                                <th width="20%">PRICE</th>
                                <th width="20%">QUANTITY</th>
                                <th width="20%">TOTAL</th>
                                <th width="20%">TOTAL AMOUNT</th>

                                <th width="20%">STATUS</th>
                                <th width="20%">CNACEL</th>
                            </tr>
                        </thead>
                        <tbody>
                            <% data.forEach(x=>{%>
                                <tr>
                                    <td>
                                        <a class="itemside" href="#">
                                            <div class="left">
                                                <img src="static/uploads/<%=x.details.product_image[0]%>" width="40"
                                                    height="40" class="img-xs" alt="Item">
                                            </div>
                                            <div class="info">
                                                <%=x.details.product_name%>
                                                    </< /div>
                                        </a>
                                    </td>
                                    <td>
                                        <%=x.details.product_price%>
                                    </td>
                                    <td>
                                        <%=x.quantity%>
                                    </td>
                                    <td>
                                        <%=x.details.product_category%>
                                    </td>
                                    <td>
                                        <%=x.details.product_price * x.quantity %>
                                    </td>
                                    <% if(x.status !="canceled" && x.status !="delivered" ){%>
                                        <td> <a href="/cancel?pid=<%=x.details._id%>&id=<%=datas._id%>&qn=<%=x.quantity%>&wh=0" style=" border: none;
                                    border-radius: 5px;
                                    color: white;
                                    cursor: pointer;
                                    font-size: 14px;
                                    padding: 8px 12px;
                                   background-color: green;">CANCEL</a></td>
                                        <%}%>

                                            <% if(x.status=="canceled" ){%>
                                                <td> <a href="#" style=" border: none;
                                        border-radius: 5px;
                                        color: white;
                                        cursor: pointer;
                                        font-size: 14px;
                                        padding: 8px 12px;
                                       background-color: rgb(7, 169, 163);">closed</a></td>
                                                <%}%>


                                                    <% if(x.status=="requested" ){%>
                                                        <td> <a href="/approve?pid=<%=x.details._id%>&id=<%=datas._id%>&qn=<%=x.quantity%>"
                                                                style=" border: none;
                                        border-radius: 5px;
                                        color: white;
                                        cursor: pointer;
                                        font-size: 14px;
                                        padding: 8px 12px;
                                       background-color: red;">Approve</a></td>
                                                        <%}%>
                                                            <% if(x.status=="approved" ){%>
                                                                <td> <a href="/delivery?pid=<%=x.details._id%>&id=<%=datas._id%>"
                                                                        style=" border: none;
        border-radius: 5px;
        color: white;
        cursor: pointer;
        font-size: 14px;
        padding: 8px 12px;
       background-color: rgb(77, 195, 18);">delivery</a></td>
                                                                <%}%>
                                                                    <% if(x.status=="delivery" ){%>
                                                                        <td> <a href="/delivered?pid=<%=x.details._id%>&id=<%=datas._id%>"
                                                                                style=" border: none;
        border-radius: 5px;
        color: white;
        cursor: pointer;
        font-size: 14px;
        padding: 8px 12px;
       background-color: rgb(18, 68, 195);">delivered</a></td>
                                                                        <%}%>
                                                                            <% if(x.status=="delivered" ){%>
                                                                                <td> <a href="/delivered?pid=<%=x.details._id%>&id=<%=datas._id%>"
                                                                                        style=" border: none;
        border-radius: 5px;
        color: white;
        cursor: pointer;
        font-size: 14px;
        padding: 8px 12px;
       background-color: rgb(206, 220, 11);">completed</a></td>
                                                                                <%}%>
                                                                                    <% if(x.status=="canceled" ){%>
                                                                                        <td> <a href="#" style=" border: none;
        border-radius: 5px;
        color: white;
        cursor: pointer;
        font-size: 14px;
        padding: 8px 12px;
       background-color: rgb(11, 213, 220);">closed</a></td>
                                                                                        <%}%>

                                </tr>
                                <%})%>
                        </tbody>
                    </table>

                    <div class="card" style="width: 18rem;">
                        <img class="card-img-top" style="height: 100px; width: 50%;" src="static/images/cash.png"
                            alt="Card image cap">
                        <div class="card-body">

                            <p class="card-text">TOTAL AMOUNT</p>
                            <h1>RS : <span style="color: red;">
                                    <%=datas.total_amount%>
                                </span></h1>
                            <!-- <%=datas._id%> -->
                        </div>
                    </div>


                </div>
            </div>
        </section>


        <footer class="main-footer font-xs " style="position:initial !important;">
            <div class="row pb-30 pt-15">
                <div class="col-sm-6">
                    <script>
                        document.write(new Date().getFullYear())
                    </script> Â©, Evara - HTML Ecommerce Template .
                </div>
                <div class="col-sm-6">
                    <div class="text-sm-end">
                        All rights reserved
                    </div>
                </div>
            </div>
        </footer>
    </main>
    <script src="static/admin/assets/js/vendors/jquery-3.6.0.min.js"></script>
    <script src="static/admin/assets/js/vendors/bootstrap.bundle.min.js"></script>
    <script src="static/admin/assets/js/vendors/select2.min.js"></script>
    <script src="static/admin/assets/js/vendors/perfect-scrollbar.js"></script>
    <script src="static/admin/assets/js/vendors/jquery.fullscreen.min.js"></script>
    <script src="static/admin/assets/js/vendors/chart.js"></script>
    <!-- Main Script -->
    <script src="static/admin/assets/js/main.js" type="text/javascript"></script>
    <script src="static/admin/assets/js/custom-chart.js" type="text/javascript"></script>
</body>


