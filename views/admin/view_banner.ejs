<body>
    <%-include('nav')-%>
    <main class="main-wrap">
        

        <section class="content-main">
            <div class="content-header">
                <div>
                    <h2 class="content-title card-title">Products List</h2>
                    <p style="color: green;"><%=er%></p>
                </div>
                <div>
                    <a href="#" class="btn btn-light rounded font-md">Export</a>
                    <a href="#" class="btn btn-light rounded  font-md">Import</a>
                    <a href="/add_banner" class="btn btn-primary btn-sm rounded">Add Products</a>
                </div>
            </div>
            <div class="card-body">
                <div class="row row-cols-1 row-cols-sm-2 row-cols-lg-3 row-cols-xl-4">
                  <!-- col.// -->
                  <% data.forEach((x)=>{%>
                    <div class="col">
                        <div class="card card-user">
                            <div class="card-header">
                                <img class="img-md img-avatar"   src="static/uploads/<%=x.banner_image%>" alt="User pic">
                            </div>
                            <div class="card-body">
                                <h5 class="card-title mt-50">Name: <%=x.banner_name%></h5>
                                <div class="card-text text-muted">
                                 
                                   
                                    <p><template class="__cf_email__" data-cfemail="7f131a0c13161a3f1a071e120f131a511c1012">[email&#160;protected]</template></p>
                                    <%if(x.in_use==false){%>
                                    <a href="/use_banner?id=<%=x._id%>" class="btn btn-sm btn-brand rounded font-sm mt-15">use Banner</a>
                                    <%}%>
                                    <%if(x.in_use==true){%>
                                        <a href="/remove_banner?id=<%=x._id%>" class="btn btn-sm btn-brand rounded font-sm mt-15">remove Banner</a>
                                     <%}%>
                                    <a href="/delete_banner?id=<%=x._id%>" class="btn btn-sm btn-brand rounded font-sm mt-15">Delete Banner</a>
                                </div>
                            </div>
                        </div>
                    </div>
                    <%})%> <!-- col.// -->
                </div> <!-- row.// -->
            </div> 
        </section>
          
        
        <footer class="main-footer font-xs " style="position:initial !important;">
            <div class="row pb-30 pt-15">
                <div class="col-sm-6">
                    <script>
                    document.write(new Date().getFullYear())
                    </script> Â©, Evara - HTML Ecommerce Template .
                </div>
                <div class="col-sm-6">
                    <div class="text-sm-end">
                        All rights reserved
                    </div>
                </div>
            </div>
        </footer>
    </main>
    <script src="static/admin/assets/js/vendors/jquery-3.6.0.min.js"></script>
    <script src="static/admin/assets/js/vendors/bootstrap.bundle.min.js"></script>
    <script src="static/admin/assets/js/vendors/select2.min.js"></script>
    <script src="static/admin/assets/js/vendors/perfect-scrollbar.js"></script>
    <script src="static/admin/assets/js/vendors/jquery.fullscreen.min.js"></script>
    <script src="static/admin/assets/js/vendors/chart.js"></script>
    <!-- Main Script -->
    <script src="static/admin/assets/js/main.js" type="text/javascript"></script>
    <script src="static/admin/assets/js/custom-chart.js" type="text/javascript"></script>
