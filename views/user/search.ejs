    <style>
		.single-product-item {
		  background-color: rgb(255, 255, 255);
		  color: rgb(0, 0, 0);
		  padding: 10px;
		  border-radius: 10px;
		}
	  
		.single-product-item h3, .single-product-item p {
		  color: rgb(8, 0, 0);
		}
	  
		/* .single-product-item .product-image img {
		  height: 150px;
		} */
	  
		.btnn1, .btnn2 {
		  color: white;
		  border-radius: 5px;
		  padding: 5px 10px;
		  transition: background-color 0.3s;
		}
	  
		.btnn1 {
		  background-color: #4CAF50;
		}
	  
		.btnn2 {
		  background-color: #f44336;
		}
	  
		.btnn1:hover, .btnn2:hover {
		  background-color: gray;
		  cursor: pointer;
		}
	  
		.single-product-item:hover {
		  transform: scale(1.1);
		  transition: transform 0.3s;
		}
	  </style>
    
	<script type="text/javascript">
		function preventBack() {
			window.history.forward(); 
		}
		  
		setTimeout("preventBack()", 0);
		  
		window.onunload = function () { null };
	</script>
</head>
<body>
	
	<!--PreLoader-->
	<%-include('nav')-%>
	<!-- end header -->

	<!-- search area -->
	<%-include('search_page')-%>
	<!-- end search arewa -->
	
	<!-- breadcrumb-section -->
	<div class="breadcrumb-section breadcrumb-bg">
		<div class="container">
			<div class="row">
				<div class="col-lg-8 offset-lg-2 text-center">
					<div class="breadcrumb-text">
						<p>Lost and Found </p>
						<h1>Shop</h1>
					</div>
				</div>
			</div>
		</div>
	</div>
	<!-- end breadcrumb section -->

	<!-- products -->
	<div class="product-section mt-150 mb-150">
		<div class="container">

			<!-- <div class="row product-lists">
                <% data.forEach(x=>{
                    if(x.product_verified==true){%>
				<div class="col-lg-4 col-md-6 text-center strawberry">
					<div class="single-product-item">
						<div class="product-image">
							<a href="#"><img style="height: 200px;" src="static/uploads/<%=x.product_image%>" alt=""></a>
						</div>
						<h3><%=x.product_name%></h3>
						<p class="product-price"><span>.....</span>Rs <%=x.product_price%></p>
						<a href="#" class="cart-btn"><i class="fas fa-shopping-cart"></i> Add to Cart</a>
					</div>
				</div>
                <%}})%>  
			</div> -->
         
            <%if(!data[0]){%>
                <h2>No Such Product</h2>   

				<%}%>
           
			<div class="row product-lists">
				<% data.forEach(x => {%>
					
						<div class="col-lg-4 col-md-6 text-center strawberry">
							<div class="single-product-item">
								<div class="product-image">
									<%if (x.product_verified == true) { %>
									<a href="/details?id=<%=x._id%>">
										<img src="static/uploads/<%=x.product_image[0]%>" alt="">
									</a>
									<%}else{%>
										<a href="#">
											<img src="static/uploads/<%=x.product_image[0]%>" alt="">
										</a>
										<%}%>
								</div>
								<h3><%=x.product_name%></h3>
								<p class="product-price">
									<span style="color: orangered;">Only <%=x.product_quantity%></span>₹ <%=x.product_price%>
								</p>
								
								<!-- <button class="btnn1"><i class="fas fa-shopping-cart"></i> Add to Cart</button> -->
								<%if (x.product_verified == true) { %>
								<a href="/details?id=<%=x._id%>">	<button class="btnn2"><i class="far fa-heart"></i>view detail</button></a>
								<%}else{%>
									<h4>not available</h4>
									<%}%>
							</div>
						</div>
						<script>
							const btnn1 = document.querySelector('.btnn1');
							const btnn2 = document.querySelector('.btnn2');
			  
							btnn1.addEventListener('mouseover', () => {
							  btnn1.style.transform = 'scale(1.1)';
							});
			  
							btnn1.addEventListener('mouseout', () => {
							  btnn1.style.transform = 'scale(1)';
							});
			  
							btnn2.addEventListener('mouseover', () => {
							  btnn2.style.transform = 'scale(1.1)';
							});
			  
							btnn2.addEventListener('mouseout', () => {
							  btnn2.style.transform = 'scale(1)';
							});
			  
							btnn1.addEventListener('click', () => {
							  // Add to cart functionality here
							});
			  
							btnn2.addEventListener('click', () => {
							  // Add to wishlist functionality here
							});
						</script>
				<%  }) %>
			  </div>
            
          
              
          
            </div>
            

			
		</div>
	</div>
